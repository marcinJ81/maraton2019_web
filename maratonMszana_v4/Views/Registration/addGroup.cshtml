@model Abstract_And_Model_Layer.Registration_Participant_Model.grupa_kolarska

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/moment-with-locales.js"></script>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <p style="font-size:22px;font-weight: bold;">
                    Dodaj nową grupę
                </p>
            </div>
            <div class="modal-body ">
                <div>
                    Nazwa Grupy
                </div>
                <div class="editor-field">
                    @Html.EditorFor(model => model.grupa_nazwa)
                    @Html.ValidationMessageFor(model => model.grupa_nazwa)
                </div>
            </div>
            <div class="modal-footer">
                <table>
                    <tr>
                        <td>
                            <input type="submit" value="Dodaj" class="btn btn-info" />
                        </td>
                        <td>
                        </td>
                        <td>
                            @Html.ActionLink("Anuluj", "Register", "Registration", null, new { @class = "btn btn-danger" })
                        </td>
                    </tr>
                </table>            
            </div>
        </div>
    </div>
</div>
}


<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/moment-with-locales.min.js"></script>
<script type="text/javascript">
    $(document).ready(function ()
    {
       // debugger
        $('#myModal').modal({ backdrop: 'static' });

        $('#klozet').on('click', function (event) {
            event.preventDefault();
           // $('#myModal').modal('close');
        });
    });
    //tu funkcja ktora wyswiela mi z powrotem liste szyn do wyslania

</script>


